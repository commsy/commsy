<div class="uk-navbar-content">
    <ul class="uk-navbar-nav">
        {#<li>#}
            {#<a href="#"><i class="uk-icon-bell-o"></i></a>#}
        {#</li>#}

        {% if showPortalConfigurationLink %}
            <li id="navbar_portal_settings_icon" class="uk-parent" data-uk-dropdown>
                <a href="#">
                    <i class="uk-icon-wrench"></i>
                </a>

                <div class="uk-dropdown uk-dropdown-small uk-dropdown-navbar">
                    <ul class="uk-nav uk-nav-navbar">
                        <li>
                            <a href="{{ path('app_portal_roomcategories', {'roomId': portal.itemId}) }}"><i class="uk-icon-justify uk-icon-wrench"></i> {{ 'Portal configuration'|trans({}, 'portal') }}</a>
                        </li>
                        <li>
                            <a href="{{ path('app_portal_legacysettings', { 'roomId': portal.itemId }) }}"><i class="uk-icon-justify uk-icon-history"></i> {{ 'Legacy configuration'|trans({}, 'portal') }}</a>
                        </li>
                    </ul>
                </div>
            </li>
        {% endif %}

        {% if supportLink is not empty and tooltip is not empty %}
            <li>
                <a href="{{ supportLink }}" title="{{ tooltip }}" target="_blank"><i class="uk-icon-question-circle"></i></a>
            </li>
        {% endif %}

        <li>
            <a href="{{ path('app_copy_list', {'roomId': roomId}) }}" title="{{ 'copies'|capitalize|trans({}, "rubric")}}">
                <i class="uk-icon-copy"></i>
                <small id="cs-nav-copy-indicator">
                {% if count > 0 %}
                    {{ count }}
                {% endif %}
                </small>
            </a>
        </li>

        {% if privateRoomItem %}
            {% if privateRoomItem.portfolioEnabled %}
                <li>
                    <a href="{{ path('app_portfolio_index', {'roomId': privateRoomItem.itemId}) }}" title="{{ 'Portfolios'|trans({}, "portfolio") }}">
                        <i class="uk-icon-table"></i>
                    </a>
                </li>
            {% endif %}

            <li>
                <a href="{{ path('app_material_list', {'roomId': privateRoomItem.itemId}) }}" title="{{ 'stackStatus'|trans({}, "profile")}}">
                    <i class="uk-icon-folder-open-o"></i>
                </a>
            </li>

            {% if privateRoomItem %}
                <li>
                    <a href="{{ path('app_dashboard_overview', {'roomId': privateRoomItem.itemId}) }}" title="{{ 'dashboard'|trans({}, "rubric")}}">
                        <i class="uk-icon-qrcode"></i>
                    </a>
                </li>
            {% endif %}
        {% endif %}
    </ul>
</div>
