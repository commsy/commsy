<?xml version="1.0"?>
<xliff version="1.2" xmlns="urn:oasis:names:tc:xliff:document:1.2">
    <file source-language="en" datatype="plaintext" original="file.ext">
        <body>
            <trans-unit id="rssDescription">
                <source>The RSS-feed keeps you up to date with everything that's new in workspace %room_title%.</source>
                <target>Der Feed informiert über alle Neuigkeiten im Raum %room_title%.</target>
            </trans-unit>
            <trans-unit id="rssNotVisible">
                <source>Not visible</source>
                <target>Nicht sichbar</target>
            </trans-unit>
            <trans-unit id="rssNewMember">
                <source>A new person has become a member.</source>
                <target>Eine neue Person ist jetzt Mitglied.</target>
            </trans-unit>
            <trans-unit id="rssProfileUpdated">
                <source>The profile has been updated.</source>
                <target>Das Benutzerprofil wurde aktualisiert.</target>
            </trans-unit>
            <trans-unit id="rssTitleDate">
                <source>Event: %title%</source>
                <target>Termin: %title%</target>
            </trans-unit>
            <trans-unit id="rssTitleMaterial">
                <source>Material: %title%</source>
                <target>Material: %title%</target>
            </trans-unit>
            <trans-unit id="rssTitleSection">
                <source>Section %title% of: %linked_title%</source>
                <target>Abschnitt %title% von: %linked_title%</target>
            </trans-unit>
            <trans-unit id="rssTitleAnnotation">
                <source>Comment %title% on: %linked_title%</source>
                <target>Kommentar %title% zu: %linked_title%</target>
            </trans-unit>
            <trans-unit id="rssTitleGroup">
                <source>Group: %title%</source>
                <target>Gruppe: %title%</target>
            </trans-unit>
            <trans-unit id="rssTitleInstitution">
                <source>Institution: %title%</source>
                <target>Institution: %title%</target>
            </trans-unit>
            <trans-unit id="rssTitleTopic">
                <source>Topic: %title%</source>
                <target>Thema: %title%</target>
            </trans-unit>
            <trans-unit id="rssTitleAnnouncement">
                <source>Announcement: %title%</source>
                <target>Ankündigung: %title%</target>
            </trans-unit>
            <trans-unit id="rssTitleTodo">
                <source>Task: %title% (due date %due_date%)</source>
                <target>Aufgabe: %title% (fällig am %due_date%)</target>
            </trans-unit>
            <trans-unit id="rssTitleStep">
                <source>Step %title% of: %linked_title%</source>
                <target>Arbeitsschritt %title% von: %linked_title%</target>
            </trans-unit>
            <trans-unit id="rssTitleDiscussion">
                <source>Discussion: %title%</source>
                <target>Diskussion: %title%</target>
            </trans-unit>
            <trans-unit id="rssTitleDiscussionArticle">
                <source>Post %title% in: %linked_title%</source>
                <target>Beitrag %title% zu: %linked_title%</target>
            </trans-unit>
        </body>
