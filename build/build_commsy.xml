<?xml version="1.0" encoding="UTF-8"?>

<project name="CommSy Dojo Build" default="build">
	
	<property environment="env"/>
	<property name="project.workspace" value="${env.WORKSPACE}"/>
	
	<target name="build" depends="clean,js"/>
	
	<target name="clean">
	</target>
	
	<target name="js">
		<echo>WORKSPACE: ${project.workspace}</echo>
		<exec executable="${project.workspace}/htdocs/js/build.sh"></exec>
	</target>
	
</project>