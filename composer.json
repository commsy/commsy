{
    "name": "commsy/commsy",
    "license": "GPLv2",
    "type": "project",
    "autoload": {
        "psr-4": {
            "": "src/"
        },
        "classmap": [
            "app/AppKernel.php",
            "app/AppCache.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^7.1.3",
        "doctrine/orm": "^2.6",
        "doctrine/doctrine-bundle": "^1.10",
        "twig/extensions": "^1.5",
        "symfony/swiftmailer-bundle": "^3.2",
        "symfony/monolog-bundle": "^3.3",
        "symfony/polyfill-apcu": "^1.10",
        "sensio/framework-extra-bundle": "^5.2",
        "incenteev/composer-parameter-handler": "^2.1",
        "doctrine/doctrine-migrations-bundle": "^2.0",
        "liip/theme-bundle": "^1.6",
        "knplabs/knp-menu-bundle":"^2.2",
        "lexik/form-filter-bundle": "^5.0",
        "craue/twigextensions-bundle": "^2.4",
        "knplabs/knp-snappy-bundle": "^1.6",
        "tomnomnom/etherpad-lite-client": "^0.1.2",
        "zendframework/zend-soap": "^2.7",
        "debril/rss-atom-bundle": "^4.2",
        "idci/color-scheme-bundle": "dev-master",
        "ci/restclientbundle": "^2.0",
        "liip/imagine-bundle": "^2.1",
        "nette/utils": "^3.0",
        "ezyang/htmlpurifier": "^4.1",
        "symfony/stopwatch": "^3.4",
        "php-imap/php-imap": "^3.0",
        "whiteoctober/breadcrumbs-bundle": "^1.4",
        "friendsofsymfony/elastica-bundle": "^5.0.3",
        "eluceo/ical": "^0.15",
        "sabre/dav": "^3.2.2",
        "mediawiki/oauthclient": "^0.1.2",
        "nategood/httpful": "^0.2.20",
        "friendsofsymfony/ckeditor-bundle": "^1.2",
        "symfony/flex": "^1.2",
        "symfony/asset": "^4.2",
        "symfony/orm-pack": "^1.0",
        "symfony/twig-bundle": "^4.2",
        "symfony/form": "^4.2",
        "symfony/security-bundle": "^4.2",
        "symfony/translation": "^4.2",
        "symfony/validator": "^4.2"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^4.2",
        "doctrine/doctrine-fixtures-bundle": "^3.1",
        "codeception/codeception": "^2.5",
        "symfony/maker-bundle": "^1.11",
        "symfony/dotenv": "^4.2",
        "symfony/profiler-pack": "^1.0"
    },
    "scripts": {
        "symfony-scripts": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "FOS\\CKEditorBundle\\Composer\\CKEditorScriptHandler::install"
        ],
        "post-install-cmd": [
            "@symfony-scripts"
        ],
        "post-update-cmd": [
            "@symfony-scripts"
        ],
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        }
    },
    "config": {
        "bin-dir": "bin"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-bin-dir": "bin",
        "symfony-var-dir": "var",
        "symfony-web-dir": "web",
        "symfony-tests-dir": "tests",
        "symfony-assets-install": "relative",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml"
        },
        "branch-alias": null,
        "ckeditor-tag": "4.6.2",
        "ckeditor-clear": "drop"
    }
}
