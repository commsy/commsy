{
    "name": "commsy/commsy",
    "description": "A web-based community system",
    "license": "GPLv2",
    "type": "project",
    "require": {
        "php": ">=8.2",
        "ext-ctype": "*",
        "ext-fileinfo": "*",
        "ext-gd": "*",
        "ext-iconv": "*",
        "ext-intl": "*",
        "ext-json": "*",
        "ext-zip": "*",
        "api-platform/core": "^2.6",
        "composer/package-versions-deprecated": "^1.8",
        "craue/twigextensions-bundle": "^2.4",
        "debril/rss-atom-bundle": "^5.0",
        "doctrine/annotations": "^2.0",
        "doctrine/doctrine-bundle": "^2.5",
        "doctrine/doctrine-migrations-bundle": "^3.2",
        "doctrine/orm": "^2.11",
        "eluceo/ical": "^2.5",
        "ezyang/htmlpurifier": "^4.1",
        "friendsofsymfony/ckeditor-bundle": "^2.0",
        "friendsofsymfony/elastica-bundle": "^6.1",
        "knplabs/knp-menu-bundle": "^3.0",
        "knplabs/knp-paginator-bundle": "^5.3",
        "knplabs/knp-snappy-bundle": "^1.7",
        "lexik/form-filter-bundle": "^7.0",
        "lexik/jwt-authentication-bundle": "^2.14",
        "liip/imagine-bundle": "^2.3",
        "masterminds/html5": "^2.7",
        "mhujer/breadcrumbs-bundle": "^1.5",
        "nategood/httpful": "^0.3",
        "nelmio/api-doc-bundle": "^4.8",
        "nelmio/cors-bundle": "^2.2",
        "nette/utils": "^3.1",
        "ozdemirburak/iris": "^2.3",
        "php-imap/php-imap": "^4.1",
        "phpdocumentor/reflection-docblock": "^5.3",
        "promphp/prometheus_client_php": "^2.6",
        "sabre/vobject": "^4.3",
        "sylius/theme-bundle": "^2.2",
        "symfony/asset": "6.2.*",
        "symfony/console": "6.2.*",
        "symfony/doctrine-messenger": "6.2.*",
        "symfony/dotenv": "6.2.*",
        "symfony/expression-language": "6.2.*",
        "symfony/flex": "^2.2",
        "symfony/form": "6.2.*",
        "symfony/framework-bundle": "6.2.*",
        "symfony/http-client": "6.2.*",
        "symfony/ldap": "6.2.*",
        "symfony/lock": "6.2.*",
        "symfony/mailer": "6.2.*",
        "symfony/messenger": "6.2.*",
        "symfony/monolog-bundle": "^3.3",
        "symfony/polyfill-apcu": "^1.10",
        "symfony/process": "6.2.*",
        "symfony/property-access": "6.2.*",
        "symfony/property-info": "6.2.*",
        "symfony/proxy-manager-bridge": "6.2.*",
        "symfony/runtime": "6.2.*",
        "symfony/security-bundle": "6.2.*",
        "symfony/serializer": "6.2.*",
        "symfony/stopwatch": "6.2.*",
        "symfony/translation": "6.2.*",
        "symfony/twig-bundle": "6.2.*",
        "symfony/ux-autocomplete": "^2.7",
        "symfony/ux-twig-component": "^2.6",
        "symfony/validator": "6.2.*",
        "symfony/webpack-encore-bundle": "^1.6",
        "symfony/workflow": "6.2.*",
        "symfony/yaml": "6.2.*",
        "tomnomnom/etherpad-lite-client": "^0.1.2",
        "twig/extra-bundle": "^3.3",
        "twig/string-extra": "^3.3",
        "vich/uploader-bundle": "^1.16"
    },
    "require-dev": {
        "codeception/codeception": "^5.0",
        "codeception/module-asserts": "^3.0",
        "codeception/module-datafactory": "^3.0",
        "codeception/module-db": "^3.0",
        "codeception/module-doctrine2": "^3.0",
        "codeception/module-filesystem": "^3.0",
        "codeception/module-phpbrowser": "^3.0",
        "codeception/module-rest": "^3.3",
        "codeception/module-symfony": "^3.1",
        "doctrine/doctrine-fixtures-bundle": "^3.3",
        "rector/rector": "^0.15",
        "symfony/maker-bundle": "^1.21",
        "symfony/web-profiler-bundle": "6.2.*"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "allow-plugins": {
            "symfony/flex": true,
            "symfony/runtime": true
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        },
        "classmap": ["legacy/classes"],
        "files": [
            "legacy/etc/cs_constants.php",
            "legacy/functions/curl_functions.php",
            "legacy/functions/date_functions.php",
            "legacy/functions/language_functions.php",
            "legacy/functions/misc_functions.php",
            "legacy/functions/security_functions.php",
            "legacy/functions/text_functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "ckeditor:install --clear=drop --tag=4.20.2": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd",
            "ckeditor:install": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": true,
            "require": "6.2.*"
        }
    }
}
