{
    "name": "commsy/commsy",
    "license": "GPLv2",
    "type": "project",
    "require": {
        "php": "^7.3.0",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ci/restclientbundle": "^2.0",
        "composer/package-versions-deprecated": "^1.8",
        "craue/twigextensions-bundle": "^2.4",
        "debril/rss-atom-bundle": "^5.0",
        "doctrine/doctrine-bundle": "^2.1",
        "doctrine/doctrine-migrations-bundle": "^3.0",
        "doctrine/orm": "^2.7",
        "eluceo/ical": "^0.15",
        "ezyang/htmlpurifier": "^4.1",
        "friendsofsymfony/ckeditor-bundle": "^2.0",
        "friendsofsymfony/elastica-bundle": "^5.2",
        "friendsofsymfony/rest-bundle": "^3.0",
        "idci/color-scheme-bundle": "dev-master",
        "knplabs/knp-menu-bundle": "^3.0",
        "knplabs/knp-paginator-bundle": "^5.3",
        "knplabs/knp-snappy-bundle": "^1.7",
        "laminas/laminas-soap": "^2.8",
        "lexik/form-filter-bundle": "^6.0",
        "lexik/jwt-authentication-bundle": "^2.8",
        "liip/imagine-bundle": "^2.3",
        "liip/theme-bundle": "^1.7",
        "masterminds/html5": "^2.7",
        "mediawiki/oauthclient": "^1.1",
        "mhujer/breadcrumbs-bundle": "^1.5",
        "nategood/httpful": "^0.3",
        "nelmio/api-doc-bundle": "^3.7",
        "nette/utils": "^3.1",
        "php-imap/php-imap": "^4.1",
        "sensio/framework-extra-bundle": "^5.6",
        "symfony/asset": "4.4.*",
        "symfony/console": "4.4.*",
        "symfony/dotenv": "4.4.*",
        "symfony/expression-language": "4.4.*",
        "symfony/flex": "^1.3",
        "symfony/form": "4.4.*",
        "symfony/framework-bundle": "4.4.*",
        "symfony/http-client": "4.4.*",
        "symfony/ldap": "4.4.*",
        "symfony/monolog-bundle": "^3.3",
        "symfony/polyfill-apcu": "^1.10",
        "symfony/process": "4.4.*",
        "symfony/security-bundle": "4.4.*",
        "symfony/serializer": "4.4.*",
        "symfony/stopwatch": "4.4.*",
        "symfony/swiftmailer-bundle": "^3.2",
        "symfony/translation": "4.4.*",
        "symfony/twig-bundle": "4.4.*",
        "symfony/validator": "4.4.*",
        "symfony/webpack-encore-bundle": "^1.6",
        "symfony/yaml": "4.4.*",
        "tomnomnom/etherpad-lite-client": "^0.1.2",
        "twig/extensions": "^1.5",
        "vich/uploader-bundle": "^1.16"
    },
    "require-dev": {
        "codeception/codeception": "^4.1",
        "dealerdirect/phpcodesniffer-composer-installer": "^0.7",
        "doctrine/doctrine-fixtures-bundle": "^3.3",
        "phpcompatibility/php-compatibility": "^9.3",
        "symfony/maker-bundle": "^1.21",
        "symfony/phpunit-bridge": "4.4.*",
        "symfony/profiler-pack": "^1.0",
        "codeception/module-webdriver": "^1.1",
        "codeception/module-db": "^1.0",
        "codeception/module-asserts": "^1.3",
        "codeception/module-symfony": "^1.1",
        "codeception/module-doctrine2": "^1.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "ckeditor:install --clear=drop": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": true,
            "require": "4.4.*"
        },
        "ckeditor-tag": "4.11.4",
        "ckeditor-clear": "drop"
    }
}
