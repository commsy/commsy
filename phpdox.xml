<phpdox xmlns="http://phpdox.net/config">
	<project name="CommSy" source="../" workdir="build/phpdox">
		<collector publiconly="false">
			<include mask="*.php" />
			<exclude mask="*Autoload.php" />
			<exclude mask="vendor" />
		</collector>

		<generator output="build">
			<build engine="html" enabled="true" output="api" />
		</generator>
	</project>
</phpdox>